type Mutation {
    createStudent(createStudentRequest : CreateStudentRequest) : StudentResponse   # we already have StudentResponse maping in query.graphqls file
}

input CreateStudentRequest {
    firstName : String
    lastName : String
    email : String
    street : String
    city : String
    subjectsLearning : [CreateSubjectRequest]   # list mapping
}

input CreateSubjectRequest {
    subjectName : String
    marksObtained : Float
}

